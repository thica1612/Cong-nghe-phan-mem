            <td>
                @Html.DisplayFor(modelItem => item.KoiGender)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.KoiYear)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.KoiSize)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.KoiPrice)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Stock)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.KoiImage)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Dailyfood)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Screeningrate)
            </td>
            <td>
                <a asp-page="./Edit" asp-route-id="@item.KoiId">Edit</a> |
                <a asp-page="./Details" asp-route-id="@item.KoiId">Details</a> |
                <a asp-page="./Delete" asp-route-id="@item.KoiId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
