            <th>
                @Html.DisplayNameFor(model => model.Koi[0].Stock)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Koi[0].KoiImage)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Koi[0].Dailyfood)
