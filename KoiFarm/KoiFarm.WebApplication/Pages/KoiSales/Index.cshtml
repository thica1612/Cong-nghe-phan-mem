    <tbody>
@foreach (var item in Model.KoiSale) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.KoiSaleName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.KoiSaleOrigin)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.KoiSaleGender)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.KoiSaleYear)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.KoiSaleSize)
