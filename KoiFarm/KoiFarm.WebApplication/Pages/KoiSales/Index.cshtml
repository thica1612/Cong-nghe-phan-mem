            <td>
                @Html.DisplayFor(modelItem => item.KoiSalePrice)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.KoiSalePriceLater)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DiscountPercent)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.StartDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Enddate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.StockKoiSale)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.KoiImageKoiSale)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DailyfoodKoiSale)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ScreeningrateKoiSale)
            </td>
            <td>
                <a asp-page="./Edit" asp-route-id="@item.KoiSaleId">Edit</a> |
                <a asp-page="./Details" asp-route-id="@item.KoiSaleId">Details</a> |
                <a asp-page="./Delete" asp-route-id="@item.KoiSaleId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
