            <th>
                @Html.DisplayNameFor(model => model.KoiSale[0].KoiSaleOrigin)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.KoiSale[0].KoiSaleGender)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.KoiSale[0].KoiSaleYear)
            </th>
